{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "RCAssistant",
    "version": "1",
    "intents": [],
    "slotTypes": [],
    "voiceId": "0",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 600,
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I did not understand, can you rephrase that?"
        }
      ],
      "maxAttempts": 5
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I could not understand. Please try again."
        }
      ]
    }
  }
}